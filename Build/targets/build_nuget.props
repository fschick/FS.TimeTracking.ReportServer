<Project>

  <PropertyGroup>
    <PackageId>$([MSBuild]::ValueOrDefault('$(AssemblyName)', '').Replace('FS.', 'Schick.'))</PackageId>
    <PackageDescription>$(Description)</PackageDescription>
    <PackageIcon>Schick.TimeTracking.ReportServer.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageProjectUrl>https://github.com/fschick/TimeTracking.ReportServer</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>$(Tags)</PackageTags>
  </PropertyGroup>

</Project>