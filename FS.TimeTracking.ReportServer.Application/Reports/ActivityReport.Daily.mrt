		public static string ToHours(TimeSpan timeSpan)
		{
			return string.Format("{0:00}:{1:00}", Math.Truncate(timeSpan.TotalHours), timeSpan.Minutes);
		}
		
		public static DateTime ToZimeZone(DateTime dateTimeUtc, string timeZoneId)
		{
